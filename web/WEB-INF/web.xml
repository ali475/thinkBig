<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ManageData</servlet-name>
        <servlet-class>ManageData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>OpinionManager</servlet-name>
        <servlet-class>OpinionManager</servlet-class>
        <init-param>
            <param-name>ID</param-name>
            <param-value>Value</param-value>
        </init-param>
        <init-param>
            <param-name>Text</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>OpinionViewer</servlet-name>
        <servlet-class>OpinionViewer</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ManageData</servlet-name>
        <url-pattern>/ManageData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OpinionManager</servlet-name>
        <url-pattern>/OpinionManager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>OpinionViewer</servlet-name>
        <url-pattern>/OpinionViewer</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
